#!/bin/sh

#
# Program: biotype_mgp.config
#
# Purpose:
#
#   Configuration file for BioTypes : MGP
#
# History:
#
# 04/25/2018    lec     
#       - TR12734/GenFevah/biotype
#

cd `dirname $0` 

RUNTIME_DIR="${DATALOADSOUTPUT}/mgi/vocload/runTimeBiotype/mgp"
ARCHIVE_DIR="${DATALOADSOUTPUT}/mgi/vocload/archiveBiotype"

export RUNTIME_DIR
export ARCHIVE_DIR

LOAD_PROGRAM="biotype.py ${LOAD_PROGRAM}"
export LOAD_PROGRAM

case `uname -n` in
bhmgiapp01) MAINTAINER="mgiadmin";;
*) MAINTAINER="$USER";;
esac
export MAINTAINER

# vocabulary attributes:

VOCAB_NAME="BioType Mouse Genome Project"
VOCAB_COMMENT_KEY=0
JNUM="J:259852"
IS_SIMPLE=1
IS_PRIVATE=0
LOGICALDB_KEY=1
ANNOT_TYPE_KEY=0

export VOCAB_NAME
export VOCAB_COMMENT_KEY
export JNUM
export IS_SIMPLE
export IS_PRIVATE
export LOGICALDB_KEY
export ANNOT_TYPE_KEY

BIOTYPE_FILE="${DATALOADSOUTPUT}/mgi/genemodelload/input/mgp.txt"
DATA_FILE="${RUNTIME_DIR}/biotype.tab"
TERM_FILE="${RUNTIME_DIR}/Termfile"
DAG_ROOT_ID=""

export BIOTYPE_FILE
export DATA_FILE
export TERM_FILE
export DAG_ROOT_ID

