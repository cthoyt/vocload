#!/bin/sh

#
# Program: IP.config
#
# Purpose:
#
#   Configuration file for Adult Mouse Anatomy Load
#
# Usage:
#
#   Execute this file from a script or command line to
#   set environment variables for IP load
#
# History:
#
#	03/25/2003	lec
#	- new Configuration file
#

cd `dirname $0` 

RUNTIME_DIR="`pwd`/runTimeIP"
ARCHIVE_DIR="`pwd`/archiveIP"

export RUNTIME_DIR
export ARCHIVE_DIR

LOAD_PROGRAM="IP.py simpleLoad.py"
RCD_FILE="simple.rcd"
MAINTAINER="lec@informatics.jax.org, blk@informatics.jax.org"

export LOAD_PROGRAM
export RCD_FILE
export MAINTAINER

# vocabulary attributes:

VOCAB_NAME="InterPro Domain"		# name of vocabulary (VOC_Vocab.name)
JNUM="J:23000"		# reference for this vocabulary
IS_SIMPLE=1		# structured vocabulary
IS_PRIVATE=0		# acc IDs are public
LOGICALDB_KEY=28	# ACC_LogicalDB._LogicalDB_key
ANNOT_TYPE_KEY=1003     # VOC_AnnotType._AnnotType_key

export VOCAB_NAME
export JNUM
export IS_SIMPLE
export IS_PRIVATE
export LOGICALDB_KEY
export ANNOT_TYPE_KEY

IP_FILE="${RUNTIME_DIR}/names.dat"
#IP_FILE="/data/downloads/ftp.ebi.ac.uk/pub/databases/interpro/names.dat"
DATA_FILE="${RUNTIME_DIR}/interpro.tab"
TERM_FILE="${RUNTIME_DIR}/Termfile"

export IP_FILE
export DATA_FILE
export TERM_FILE

