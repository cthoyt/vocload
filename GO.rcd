#
# Progam:  GO.rcd
#
# Purpose:
#
#	Configuration file for GO Vocabulary load
#	which defines the GO DAGs
#
# Usage:
#
#	Used by Python Configuration module
#
# History:
#
#	03/25/2003	lec
#	- moved simple vocab variables to GO.config
#

RUNTIME_DIR = /data/loads/mgi/vocload/runTimeGO
DATA_DIR = /data/downloads/genome-ftp.stanford.edu/pub/go/ontology

[
        NAME = Molecular Function
        ABBREV = F
        ONTOLOGY_FILE = ${DATA_DIR}/function.ontology
        LOAD_FILE = ${RUNTIME_DIR}/dag.f
]
[
        NAME = Biological Process
        ABBREV = P
        ONTOLOGY_FILE = ${DATA_DIR}/process.ontology
        LOAD_FILE = ${RUNTIME_DIR}/dag.p
]
[
        NAME = Cellular Component
        ABBREV = C
        ONTOLOGY_FILE = ${DATA_DIR}/component.ontology
        LOAD_FILE = ${RUNTIME_DIR}/dag.c
]

