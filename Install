#!/bin/sh

cd `dirname $0`
. ./Configuration

# Compile Python Programs
echo "Compiling python programs..."
python -c 'import compileall; compileall.compile_dir(".")'

#
# Create the runtime files if they don't exist
#
if [ ! -d /data/loads/mgi/vocload/archiveMouseCyc ]
then
    mkdir -p /data/loads/mgi/vocload/archiveMouseCyc
fi
if [ ! -d /data/loads/mgi/vocload/runTimeMouseCyc ]
then
    mkdir -p /data/loads/mgi/vocload/runTimeMouseCyc
fi

# Set mode
echo "Setting modes..."
chmod 775 *

