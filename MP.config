#!/bin/sh

#
# Program: MP.config
#
# Purpose:
#
#   Configuration file for Mammalian Phenotype Load
#
# Usage:
#
#   Execute this file from a script or command line to
#   set environment variables for MP load
#
# History:
#
#	03/25/2003	lec
#	- new Configuration file
#

cd `dirname $0` 

RUNTIME_DIR="`pwd`/runTimeMP"
ARCHIVE_DIR="`pwd`/archiveMP"

export RUNTIME_DIR
export ARCHIVE_DIR

LOAD_PROGRAM="GOload.py"
RCD_FILE="MP.rcd"
MAINTAINER="lec@informatics.jax.org, csmith@informatics.jax.org"

export LOAD_PROGRAM
export RCD_FILE
export MAINTAINER

TRDIR="/mgi/all/wts_projects/3800/3809"
TRONTFILE="MPheno.ontology"
TRDEFSFILE="MP.defs"

export TRDIR
export TRONTFILE
export TRDEFSFILE

# vocabulary attributes:

VOCAB_NAME="Mammalian Phenotype"		# name of vocabulary (VOC_Vocab.name)
ACC_PREFIX="MP"		# acc ID prefix
JNUM="J:72460"		# reference for this vocabulary
IS_SIMPLE=0		# structured vocabulary; should be a .rcd file
IS_PRIVATE=0		# acc IDs are public
LOGICALDB_KEY=34	# ACC_LogicalDB._LogicalDB_key
ANNOT_TYPE_KEY=0        # VOC_AnnotType._AnnotType_key

export VOCAB_NAME
export ACC_PREFIX
export JNUM
export IS_SIMPLE
export IS_PRIVATE
export LOGICALDB_KEY
export ANNOT_TYPE_KEY

TERM_FILE="${RUNTIME_DIR}/Termfile"
DEFS_FILE="${RUNTIME_DIR}/MP.defs"

export TERM_FILE
export DEFS_FILE

